 \frametitle{Fórmulas Matemáticas}
    \begin{itemize}
     \item É possível criar uma equação de múltiplas linhas através do ambiente \verb|align| \\
                     \verb|\begin{align}|\\
                     \verb|f(x) & = 2x^{5} + 3x^{4} + x^{3}\nonumber\\|\\
                     \verb|& \qquad + 2x^{2} + 5x + 8 \\|\\
                     \verb|& = g(x) - h(x)|\\
                     \verb|\end{align}|\\  
     produz \begin{align}
              f(x) & = 2x^{5} + 3x^{4} + x^{3}\nonumber\\
              & \qquad + 2x^{2} + 5x + 8 \\
              & = g(x) - h(x)
            \end{align} 
    \end{itemize}
\end{frame}

\begin{frame}[fragile]
 \frametitle{Construindo equações matemáticas}
  \begin{itemize}
   \item Para criar expoentes e sub-índices, utilizamos os comandos \verb|^| e \verb|_|,
respectivamente;
   \item Exemplo: Escrevendo \\
   \verb|\begin{equation*}|\\
   \verb|\sum_{i = 1}^{n}|\\
   \verb|\end{equation*}|\\
obtemos \\
     \begin{equation*}
      \sum_{i = 1}^{n}
     \end{equation*}
  \end{itemize}
\end{frame}

\begin{frame}[fragile]
 \frametitle{Construindo equações matemáticas}
  \begin{itemize}
   \item Frações são criadas utilizando os comandos \verb|\frac{numerador}{denominador}| 
   e raízes com \verb|\sqrt[n]{radicando}|;
   \item Exemplo: Escrevendo \\
   \verb|\begin{equation*}|\\
   \verb|\frac{\sqrt[3]{xy}}{2}\quad , \quad\frac{\sqrt{xy}}{2}|\\
   \verb|\end{equation*}|\\
obtemos \\
     \begin{equation*}
      \frac{\sqrt[3]{xy}}{2} \quad , \quad \frac{\sqrt{xy}}{2}
     \end{equation*}
  \end{itemize}
\end{frame}

\begin{frame}[fragile]
 \frametitle{Construindo equações matemáticas}
  \begin{itemize}
   \item Para escrever limites, escrevemos os comandos 
\verb|\lim| ;
   \item Exemplo: Escrevendo \\
   \verb|\begin{equation*}|\\
   \verb|\lim_{x\to 0} \frac{\sin x}{x} = 1|\\
   \verb|\end{equation*}|\\
obtemos \\
     \begin{equation*}
      \lim_{x\to 0} \frac{\sin x}{x} = 1
     \end{equation*}
  \end{itemize}
\end{frame}

\begin{frame}[fragile]
 \frametitle{Construindo equações matemáticas}
